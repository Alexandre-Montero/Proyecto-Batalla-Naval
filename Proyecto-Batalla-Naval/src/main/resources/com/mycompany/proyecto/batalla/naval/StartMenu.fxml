<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.proyecto.batalla.naval.StartMenuController">
   <children>
      <Button fx:id="btnPlay" alignment="CENTER" contentDisplay="CENTER" layoutX="350.0" layoutY="500.0" mnemonicParsing="false" onAction="#switchToModeMenu" prefHeight="45.0" prefWidth="95.0" text="Jugar" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="21.0" />
         </font>
      </Button>
      <Button fx:id="btnProgramInformation" alignment="CENTER" contentDisplay="CENTER" layoutX="520.0" layoutY="510.0" mnemonicParsing="false" onAction="#switchToProgramInfo" prefHeight="35.0" prefWidth="115.0" text="Acerca de" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Button>
      <Label fx:id="textGameName" alignment="CENTER" contentDisplay="CENTER" layoutY="15.0" prefHeight="50.0" prefWidth="800.0" style="-fx-alignment: black;" text="Battleship" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnRegisterNames" alignment="CENTER" contentDisplay="CENTER" layoutX="90.0" layoutY="510.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="190.0" text="Registrar Usuarios">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
